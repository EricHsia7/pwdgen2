{
  "name": "babel-preset-modules",
  "version": "0.0.2",
  "description": "nothing to see here",
  "main": "dist/index.js",
  "scripts": {
    "start": "concurrently -r 'npm:watch:* -s'",
    "build": "babel src -d dist --ignore '**/*.test.js'",
    "test": "eslint src test && jest --colors",
    "test:browser": "cd test/browser && karmatic --no-coverage --browsers chrome:headless,sauce-chrome-61,sauce-firefox-60,sauce-safari-10,sauce-safari-11,sauce-edge-16,sauce-edge-17 '**/*.js'",
    "test:local": "cd test/browser && karmatic --no-coverage '**/*.js'",
    "test:safari": "npm run test:local -- --browsers sauce-safari-10",
    "test:edge": "npm run test:local -- --browsers sauce-edge-16",
    "watch:test": "jest --watch",
    "watch:build": "npm run -s build -- -w"
  },
  "keywords": [
    "babel",
    "modern",
    "modules",
    "ES Modules",
    "mjs",
    "transpiler"
  ],
  "files": [
    "src",
    "dist"
  ],
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "loose": true,
          "targets": {
            "node": 8
          }
        }
      ]
    ],
    "plugins": [
      "babel-plugin-add-module-exports"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "src",
      "test"
    ]
  },
  "eslintConfig": {
    "extends": "developit",
    "rules": {
      "no-console": 0,
      "new-cap": 0
    }
  },
  "eslintIgnore": [
    "test/fixtures/**/*"
  ],
  "author": "Jason Miller <jason@developit.ca>",
  "license": "Apache 2.0",
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.5",
    "acorn-jsx": "^5.0.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-add-module-exports": "^1.0.2",
    "chalk": "^2.4.2",
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-developit": "^1.1.1",
    "gzip-size": "^5.1.1",
    "if-env": "^1.0.4",
    "jest": "^24.8.0",
    "karmatic": "^1.4.0",
    "pretty-bytes": "^5.2.0",
    "rollup": "^1.16.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-node-resolve": "^5.2.0",
    "terser": "^4.0.2",
    "webpack": "^4.35.0"
  },
  "dependencies": {
    "@babel/plugin-proposal-unicode-property-regex": "^7.4.4",
    "@babel/plugin-transform-dotall-regex": "^7.4.4",
    "@babel/types": "^7.4.4",
    "esutils": "^2.0.2"
  }
}
